      "name": "miniosecret",
      "user_key": "username",
      "passwd_key": "password"
    }
},
  "db": {
    "type": "postgreSQL",
    "address": "10.67.134.32:12202",
    "name": "neursafe_fl",
    "secret_key_ref": {
      "name": "pgsecret",
      "user_key": "username",
      "passwd_key": "password"
